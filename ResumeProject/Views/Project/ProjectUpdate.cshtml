@model ResumeProject.Models.TBLProject
@{
    ViewBag.Title = "ProjectUpdate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    @Html.HiddenFor(x=>x.ProjectId, new {@class="form-control"})
    <label>Başlık</label>
   @Html.TextBoxFor(x=>x.ProjectTitle, new {@class="form-control"})
    <label>Tanım</label>
    @Html.TextBoxFor(x=>x.ProjectDescription, new {@class="form-control"})
    <label>Resim</label>
    @Html.TextBoxFor(x=>x.ProjectImageUrl, new {@class="form-control"})
    <br />
    <button class="btn btn-outline-success">Ekle</button>
</form>

